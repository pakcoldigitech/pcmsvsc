{
    "name": "pcmsvsc",
    "publisher": "pakcoldigitech",
    "displayName": "Pakuranga College",
    "description": "Pakuranga College Managed Settings for VSCode",
    "version": "0.0.10",
    "categories": ["Education"],
    "keywords": ["Pakuranga College"],
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#77c4d6",
        "theme": "dark"
    },
    "qna": false,
    "repository": "github:pakcoldigitech/pcmsvsc",
    "license": "CC-BY-4.0",
    "engines": { "vscode": "^1.65.0" },
    "extensionDependencies": [
        "CelianRiboulet.webvalidator",
        "jacobcassidy.css-nesting-syntax-highlighting",
        "mhutchie.git-graph",
        "ms-python.python",
        "ms-vscode.hexeditor",
        "ritwickdey.LiveServer",
        "lucasnorgaard.live-server-qr",
        "samuelcolvin.jinjahtml"
    ],
    "contributes": {
        "configurationDefaults": {
            "editor.bracketPairColorization.enabled": false,
            "emmet.includeLanguages": {
                "jinja-html": "html"
            },
            "explorer.confirmDragAndDrop": false,
            "files.associations": {
                "**/templates/*.html": "jinja-html"
            },
            "html.autoClosingTags": false
        },
        "keybindings": [
            {
                "key": "insert",
                "command": "-editor.action.toggleOvertypeInsertMode"
            }
        ]
    }
}
